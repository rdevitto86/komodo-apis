{
  "name": "@komodo/forge-apis-node",
  "version": "1.0.0",
  "description": "An internal library for Komodo APIs built using Node",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./aws": {
      "types": "./dist/aws/index.d.ts",
      "import": "./dist/aws/index.js"
    },
    "./config": {
      "types": "./dist/config/index.d.ts",
      "import": "./dist/config/index.js"
    },
    "./crypto": {
      "types": "./dist/crypto/index.d.ts",
      "import": "./dist/crypto/index.js"
    },
    "./db": {
      "types": "./dist/db/index.d.ts",
      "import": "./dist/db/index.js"
    },
    "./domains": {
      "types": "./dist/domains/index.d.ts",
      "import": "./dist/domains/index.js"
    },
    "./entitlements": {
      "types": "./dist/entitlements/index.d.ts",
      "import": "./dist/entitlements/index.js"
    },
    "./feature-flags": {
      "types": "./dist/feature-flags/index.d.ts",
      "import": "./dist/feature-flags/index.js"
    },
    "./logging": {
      "types": "./dist/logging/index.d.ts",
      "import": "./dist/logging/index.js"
    },
    "./middleware": {
      "types": "./dist/middleware/index.d.ts",
      "import": "./dist/middleware/index.js"
    },
    "./observability": {
      "types": "./dist/observability/index.d.ts",
      "import": "./dist/observability/index.js"
    },
    "./security": {
      "types": "./dist/security/index.d.ts",
      "import": "./dist/security/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build",
    "prepublishOnly": "npm run rebuild",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "komodo",
    "internal",
    "lib",
    "apis",
    "node"
  ],
  "author": "Komodo",
  "license": "ISC",
  "devDependencies": {
    "@eslint/json": "^0.14.0",
    "@types/node": "^25.0.2",
    "eslint": "^9.39.2",
    "globals": "^16.5.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0"
  }
}